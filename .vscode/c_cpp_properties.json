{
  "version": 4,
  "configurations": [
    {
      "name": "gcc",
      "intelliSenseMode": "windows-gcc-arm",
      "compilerPath": "${env:MBED_GCC_ARM_PATH}/arm-none-eabi-g++.exe",
      "cStandard": "gnu17",
      "cppStandard": "gnu++17",
      "forcedInclude": [
        "lv_conf.h",
        "lvgl/demos/lv_demos.h"
      ],
      "includePath": [
        "${workspaceRoot}",
        "Core/**",
        "Drivers/**",
        "lvgl/**"
      ],
      "defines": [
        "USE_HAL_DRIVER",
        "STM32F7",
        "STM32F769xx",
        "DEBUG",
        "USE_FULL_ASSERT"
      ],
      "browse": {
        "path": [
          "${workspaceRoot}"
        ],
        "limitSymbolsToIncludedHeaders": false,
        "databaseFilename": ""
      }
    }
  ]
}